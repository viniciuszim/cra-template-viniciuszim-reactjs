{
  "name": "linos-automation-packstation",
  "homepage": "http://localhost:3001/",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^12.8.0",
    "yarn": "^1.15.2"
  },
  "scripts": {
    "dev": "PORT=3001 BROWSER=none react-scripts start",
    "start": "react-scripts start",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.tsx\"",
    "lint": "eslint \"src/**/*.tsx\"",
    "lint:fix": "eslint --fix \"src/**/*.tsx\"",
    "test": "jest --maxWorkers=4 --silent",
    "test:cov": "jest --maxWorkers=4 --silent --coverage",
    "test:silent": "jest --silent",
    "test:simple": "jest --detectOpenHandles",
    "build": "react-scripts build",
    "prebuild": "rm -rf ./build && rm -rf ./cypress/videos",
    "storybook": "start-storybook -p 6006 -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.8.6",
    "@babel/helper-builder-react-jsx-experimental": "^7.9.5",
    "@babel/helper-validator-identifier": "^7.9.5",
    "@babel/plugin-transform-react-jsx-development": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "7.8.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-loader": "8.1.0",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-expo": "^8.1.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4",
    "jest":  "24.9.0",
    "metro-react-native-babel-preset": "0.58.0",
    "prettier": "^2.0.5",
    "typescript": "~3.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test --bail"
    }
  },
  "lint-staged": {
    "*.tsx": [
      "yarn format",
      "yarn lint:fix",
      "yarn test --findRelatedTests --bail"
    ]
  }
}
